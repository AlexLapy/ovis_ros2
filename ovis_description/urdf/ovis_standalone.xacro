      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0" />
    </collision>
        
    <inertial>
      <mass value="0.41555" />
      <origin rpy="0 0 0" xyz="0.0 0.03086454 -0.00810577" />
      <inertia ixx="0.00055858" ixy="-4.3e-007" ixz="-2.8e-007" iyy="0.00020049" iyz="9.39e-005" izz="0.00057545" />
    </inertial>

